            "displayName": "static-qtquick",
            "generator": "Ninja",
            "binaryDir": "${sourceDir}/build-static-qtquick",
            "cacheVariables": {
                "CMAKE_BUILD_TYPE": "Release",
                "KDDockWidgets_STATIC": "ON",
                "KDDockWidgets_QTQUICK": "ON"
